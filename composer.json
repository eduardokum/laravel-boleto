{
    "name": "r2soft/laravel-boleto",
    "description": "Biblioteca com boletos para o laravel",
    "keywords": [
        "r2soft",
        "eduardokum",
        "laravel-boleto"
    ],
    "homepage": "https://github.com/eduardokum/laravel-boleto",
    "license": "MIT",
    "authors": [
        {
            "name": "Eduardo Gusmão",
            "email": "eduguscontra3@hotmail.com"
        },
        {
            "name": "R2 Soft Infomática e Softwares LTDA",
            "email": "ricardo@r2soft.com.br"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "laravel/framework": "^5.0||^6.0||^7.0",
        "neitanod/forceutf8": "^2.0",
        "setasign/fpdf": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.0,<9.0",
        "orchestra/testbench": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Eduardokum\\LaravelBoleto\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eduardokum\\LaravelBoleto\\Tests\\": "tests"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.0"
        }
    }
}
