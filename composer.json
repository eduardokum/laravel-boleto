{
    "name": "samuelcsantos/laravel-boleto",
    "description": "Biblioteca com boletos para o laravel",
    "keywords": [
        "samuelcsantos",
        "laravel-boleto"
    ],
    "homepage": "https://github.com/samuelcsantos/laravel-boleto",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Santos",
            "email": "prof.samuel.uniderp@gmail.com"
        }
    ],
    "require": {
        "php":                ">5.5.0",
        "ext-intl":           "*",
        "ext-mbstring":       "*",
        "laravel/framework":  "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "neitanod/forceutf8": "^2.0",
        "setasign/fpdf":      "^1.8",
        "ext-curl":           "*",
        "ext-json":           "*",
        "ext-openssl":        "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0",
        "orchestra/testbench": "^3.0|^4.0|^5.0|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "samuelcsantos\\LaravelBoleto\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "samuelcsantos\\LaravelBoleto\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": true
    }
}